class Solution {
    public int sumOfSquares(int[] nums) {
        int a=nums.length;
        int count=0;
       // System.out.println(a);
        for(int i=1;i<=a;i++){
            if(a%i==0){
               // System.out.println(nums[i]);
                count=count+(nums[i-1]*nums[i-1]);
            }
        }
        return count;
    }
}
